<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
  <title>Gitlab Oauth</title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
  <!-- load bulma css -->
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  <!-- load fontawesome -->
  <style>
    html {
    position: relative;
    min-height: 100%;
  }

  body {
    background: rgb(220, 220, 220);
    font-family: Arial, Helvetica, sans-serif;
  }

  .jumbotron{
    position: relative;
    height: 530px;
  }

  .nav li {
    padding: 10px;
  }

  </style>
</head>
<header>
  <%- include('./partials/header'); %>
</header>
<body>
  <div class="container">
    <div class="jumbotron">
      <h1 class="text-primary  text-center"><span class="fa fa-gitlab"></span> User Profile</h1>
      <div class="row">
        <div class="col-sm-6">
          <div class="well">
            <p>
              <strong>Name</strong>:<img class="img-gitlab" src="avatar.png" width="40" title="gitlab_avatar" /> <%= userData.name %> <br>
                <strong>Username</strong>: <%= userData.username %><br>
                  <strong>User ID</strong>: <%= userData.id %><br>
                    <strong>Primary Email: </strong>: <%= userData.email %><br>
                      <strong>Last activity on: </strong>: <%= userData.last_activity_on %>
            </p>
          </div>
        </div>
      </div>
    </div>
  </div>
</body>

</html>